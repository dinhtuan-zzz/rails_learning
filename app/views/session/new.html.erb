<h1>Sign_in</h1>
<%= form_with url: sign_in_path, local: true do |form| %>
    <div class="mb-3">
        <%= form.label :email %>
        <%= form.text_field :email, class: "form-control", placeholder: "123@gmail.com"%>
    </div>
    <div class="mb-3">
        <%= form.label :password %>
        <%= form.password_field :password, class: "form-control", placeholder: "password" %>
    </div>
    <div class="mb-3">
        <%= form.submit "Sign in", class: "btn btn-primary"%>
    </div>
<% end %>